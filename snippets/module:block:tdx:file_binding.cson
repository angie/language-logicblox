'.source.logic':
  'Create a new tdx file binding block using module syntax':
    'prefix': 'block_tdx_file_binding'
    'body': 'block(`${1:blockname}) {\n\n \talias_all(`lb:web:delim:schema),\n \talias_all(`lb:web:delim:schema_abbr),\n \talias_all(`lb:web:delim:binding),\n \talias_all(`lb:web:delim:binding_abbr),\n\n\tclauses(`{$0\n\t\t// File definition for entity\n\t\tfile_definition_by_name["entity"] = fd,\n\t\tfile_definition(fd) {\n\t\t\tfile_delimiter[] = "|",\n\t\t\tcolumn_headers[] = headers,\n\t\t\tcolumn_formats[] = formats,\n\t\t\tfile_columns_required[] = required\n\t\t\t// file_columns_can_be_absent[] = xxx // PERFORMANCE NOTE: avoid enabling this\n\t\t} <-\n\t\t\trequired =\n\t\t\t\t"Id," +\n\t\t\t\t"Value",\n\n\t\t\theaders = \n\t\t\t\t"ID," +\n\t\t\t\t"VALUE",\n\n\t\t\tformats = \n\t\t\t\t"string," +\n\t\t\t\t"datetime(\'%Y-%m-%d %H:%M:%S%Q\')".\n    \n\t\tfile_binding_by_name["entity"] = fb, \n\t\tfile_binding(fb) {\n\t\t  file_binding_definition_name[] = "entity",\n\n\t\t  predicate_binding_by_name["entity_value"] =\n\t\t    predicate_binding(_) {\n\t\t      predicate_binding_columns[] = "Id,Value"\n\t\t    }\n\t\t}.\n\t})\n\n} <-- .'